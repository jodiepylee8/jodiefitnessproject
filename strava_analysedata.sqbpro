    type,
    COUNT(*) AS num_activities,
    SUM(distance) AS total_distance,
    SUM(elapsed_time) AS total_duration
FROM activities
GROUP BY type;

WITH monthly_totals AS ( 
	SELECT strftime('%Y-%m',start_time) AS month,
						type AS activity_type,
						SUM(distance) AS monthly_distance
	FROM activities
	GROUP BY month, activity_type
)

SELECT activity_type,
					month,
					monthly_distance
FROM monthly_totals mt1
WHERE monthly_distance = (
	SELECT MAX(monthly_distance)
	FROM monthly_totals mt2
	WHERE mt2.activity_type = mt1.activity_type
)

ORDER BY activity_type;

SELECT strftime('%H', start_time) AS hour,
       COUNT(*) AS num_activities
FROM activities
GROUP BY hour
ORDER BY num_activities DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
